{
  "name": "tree-sitter-dotvvm",
  "version": "0.1.0",
  "description": "Tree-sitter based parser of DotVVM HTML-like markup",
  "main": "bindings/node",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tree-sitter generate && tsc -p . && node-gyp configure --silent && node-gyp build --silent && yarn run build-types",
    "test": "tree-sitter generate && yarn run build-types && tree-sitter test",
    "build-types": "node ./build/src/build-typescript-nodes.js ./src > src/node-types.d.ts"
  },
  "typings": "./src/node-types.d.ts",
  "dependencies": {
    "@tsconfig/node16": "^1.0.3",
    "nan": "^2.16.0",
    "tree-sitter": "^0.20.0"
  },
  "devDependencies": {
    "@asgerf/dts-tree-sitter": "^0.1.0",
    "@types/node": "^18.6.4",
    "node-gyp": "^9.1.0",
    "tree-sitter-cli": "^0.20.6",
    "tree-sitter-javascript": "^0.19.0",
    "typescript": "^4.8.2"
  },
  "tree-sitter": [{
    "scope": "source.dotvvm",
    "file-types": [ "dothtml", "dotcontrol", "dotmaster", "dotlayout" ],
    "highlights": [ "queries/highlights.scm" ],
    "content-regex": "^@viewModel "
  }]
}
