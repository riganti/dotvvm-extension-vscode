{
    "name": "dotvvm-language-server",
    "version": "0.1.0",
    "description": "A Javascript language server for DotVVM",
    "main": "dist/src/index.js",
    "typings": "dist/src/index",
    "scripts": {
        "test": "cross-env TS_NODE_TRANSPILE_ONLY=true mocha --require test/testinit.js --require ts-node/register \"test/**/*.ts\" --exclude \"test/**/*.d.ts\"",
        "build": "tsc && pkg . --targets host",
        "debug-build": "tsc",
        "debug-watch": "tsc --watch",
        "prepublishOnly": "npm run build",
        "watch": "tsc -w"
    },
    "bin": {
        "dotvvmserver": "dist/startServer.js"
    },
    "pkg": {
        "assets": [
            "node_modules/lib-dotvvm-spy/build/Release/libDotvvmSpy.so",
            "node_modules/lib-dotvvm-spy/build/Release/dotnet-analyzer-lib-node.node",
            "node_modules/web-tree-sitter/tree-sitter.wasm",
            "node_modules/tree-sitter-dotvvm/tree-sitter-dotvvm.wasm",
            "./data/*.json",
            "./data/*.xml"
        ],
        "outputPath": "dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/riganti/dotvvm-extension-vscode.git"
    },
    "keywords": [
        "dotvvm",
        "vscode",
        "editor",
        "language-server"
    ],
    "contributors": [
        {
            "name": "DotVVM team and contributors"
        },
        {
            "name": "James Birtles",
            "email": "jameshbirtles@gmail.com"
        },
        {
            "name": "Svelte Language Tools contributors"
        }
    ],
    "license": "Apache-2.0",
    "engines": {
        "node": ">= 12.0.0"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.0",
        "@types/estree": "^0.0.42",
        "@types/lodash": "^4.14.116",
        "@types/mocha": "^9.1.0",
        "@types/node": "^18.7.21",
        "@types/prettier": "^2.2.3",
        "@types/sinon": "^7.5.2",
        "cross-env": "^7.0.2",
        "mocha": "^9.2.0",
        "pkg": "^5.8.0",
        "sinon": "^11.0.0",
        "ts-node": "^10.0.0",
        "why-is-node-running": "^2.2.2"
    },
    "dependencies": {
        "@jridgewell/trace-mapping": "^0.3.9",
        "chokidar": "^3.4.1",
        "estree-walker": "^2.0.1",
        "fast-glob": "^3.2.7",
        "lib-dotvvm-spy": "file:../native-lib-dotvvm-spy/node_binding",
        "lodash": "^4.17.21",
        "tree-sitter-dotvvm": "portal:../tree-sitter-dotvvm",
        "tree-sitter-javascript": "^0.19.0",
        "typescript": "^4.7.4",
        "vscode-css-languageservice": "~5.1.0",
        "vscode-emmet-helper": "~2.6.0",
        "vscode-html-languageservice": "~4.1.0",
        "vscode-languageserver": "8.0.2",
        "vscode-languageserver-protocol": "3.17.1",
        "vscode-languageserver-types": "3.17.2",
        "vscode-uri": "~3.0.3",
        "web-tree-sitter": "^0.20.5"
    }
}
